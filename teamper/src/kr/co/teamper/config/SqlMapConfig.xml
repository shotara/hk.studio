<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "mybatis-3-config.dtd">

<configuration>

    <typeAliases>
		<typeAlias type="com.honbabin.model.domain.Board" alias="board"/>
		<typeAlias type="com.honbabin.model.domain.MeetingEpilogue" alias="epilogue"/>
		<typeAlias type="com.honbabin.model.domain.Follow" alias="follow"/>
		<typeAlias type="com.honbabin.model.domain.Meeting" alias="meeting"/>		
		<typeAlias type="com.honbabin.model.domain.Member" alias="member"/>
		<typeAlias type="com.honbabin.model.domain.Mileage" alias="mileage"/>
		<typeAlias type="com.honbabin.model.domain.Payment" alias="payment"/>
		<typeAlias type="com.honbabin.model.domain.Place" alias="place"/>
		<typeAlias type="com.honbabin.model.domain.Univ" alias="univ"/>		
		<typeAlias type="com.honbabin.model.domain.Refund" alias="refund"/>
		<typeAlias type="com.honbabin.model.domain.Item" alias="item"/>
		<typeAlias type="com.honbabin.model.domain.Upload" alias="upload"/>
		<typeAlias type="com.honbabin.model.domain.Flashing" alias="flashing"/>
		<typeAlias type="com.honbabin.model.domain.Zone" alias="zone"/>
		<typeAlias type="com.honbabin.model.domain.Food" alias="food"/>
		<typeAlias type="com.honbabin.model.domain.Notice" alias="notice"/>	
	</typeAliases>
	
	<environments default="env">
		<environment id="env">
			<transactionManager type="JDBC" />
			<dataSource type="POOLED">
				<property name="driver" value="com.mysql.jdbc.Driver" />
				<property name="url" value="a"/>
				<property name="username" value="a" />
				<property name="password" value="a" />
			</dataSource>
		</environment>
	</environments>

	<mappers>
		<mapper resource="com/honbabin/config/board.xml" />
		<mapper resource="com/honbabin/config/epilogue.xml" />
		<mapper resource="com/honbabin/config/follow.xml" />
		<mapper resource="com/honbabin/config/meeting.xml" />
		<mapper resource="com/honbabin/config/member.xml" />
		<mapper resource="com/honbabin/config/mileage.xml" />
		<mapper resource="com/honbabin/config/payment.xml" />
		<mapper resource="com/honbabin/config/place.xml" />
		<mapper resource="com/honbabin/config/univ.xml" />
		<mapper resource="com/honbabin/config/refund.xml" />
		<mapper resource="com/honbabin/config/item.xml" />
		<mapper resource="com/honbabin/config/upload.xml" />
		<mapper resource="com/honbabin/config/flashing.xml" />
		<mapper resource="com/honbabin/config/zone.xml" />
		<mapper resource="com/honbabin/config/food.xml" />
		<mapper resource="com/honbabin/config/notice.xml" />		
	</mappers>

</configuration>

